<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Generic List // Grocery // ToDo // Goals // Whatever</title>

  <!-- Bootstrap core CSS -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO"
    crossorigin="anonymous">

  <!-- Vue Core JS -->
  <!-- development version, includes helpful console warnings -->
  <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>

  <!-- Custom styles for this template -->
  <link href="/static/form_styles.css" rel="stylesheet">
  <link href='https://fonts.googleapis.com/css?family=Lato:400,400italic,700' rel='stylesheet' type='text/css'>
</head>
<body>
<script src="/static/requests.js"></script>

<div id="haha"></div>

<div class="container">
  <h1>This Generic List</h1>

    <div id="app3">
        <div class="row">
            <div class="col text-center gap">

                <label for="item" class="item">
                    <input @keyup.enter="addItem(itemx)" name="item" v-model="itemx" type="text" id="item" placeholder=" ">
                    <span class="label">Enter Item:</span>
                    <span class="border"></span>
                  </label>

              </div>
          </div>

      <div class="row" v-for="item in note">
          <div class="col text-right">
              <div class="delete-button">
                  <div class="confirm">Confirm Delete</div>
                  <div class="button-face text-center" v-on:click="deleteItem(item._id)">Delete</div>
                </div>
          </div>
          <div class="col lists">
              {{ item.item }}
          </div>
        </div>
    </div>
    <div class="footsies">
        Bringing the rain since<br>
        (1999)
      </div>
</div>

    <script src="/static/vueapp.js"></script>

</div>
</body>

</html>